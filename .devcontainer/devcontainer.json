{
    "name": "Django app",
    // Dockerfileでイメージ・コンテナを作成
    "dockerComposeFile": "docker-compose.yml",
    // ワークスペースフォルダ
    "workspaceFolder": "/workspace",
    "service": "web",
    // リモート先のVScodeにインストールする拡張機能
    "extensions": [
        "ms-python.python"
    ],
    "shutdownAction": "stopCompose"
}